@model HalloDocWebRepository.ViewModel.sendorder<div class="container " id="opensendorderpage">    <div class="row">        <div class="col-12">            <div class="d-flex justify-content-between mt-3">                <h4 class="mt-3">Send Order</h4>                <a class="text-start text-info text-decoration-none ">                    <button class="btn btn-white  text-info border-info rounded ">                        <i class="fa-solid  fa-less-than"></i> Back                    </button>                </a>            </div>            <form asp-action="Ordersend" asp-controller="Admin" method="post">                <div class="container rounded bg-white shadow py-3">                    <div class="row">                        <div class="col-sm-12 col-md-6 col-xl-6 form-floating mb-3 text-muted">                            <div class="form-floating mb-3">                                <select class="form-select" id="ProfessionSelect">                                    <option value="0">select</option>                                    @foreach (var item in Model.healthprofessionaltype)
                                    {                                        <option value="@item.Healthprofessionalid">@item.Professionname</option>                                    }                                </select>                                <label for="ProfessionSelect">Select Profession</label>                            </div>                        </div>                        <div class="col-sm-12 col-md-6 col-xl-6 form-floating mb-3 text-muted">                            <div class="form-floating mb-3">                                <select class="form-select" id="BusinessSelect" name="SelectedVendorId">                                    @* <option value="0">select</option> *@                                    @foreach (var item in Model.healthprofessional)
                                    {                                        <option value="@item.Vendorid">@item.Vendorname</option>                                    }                                </select>                                <label for="BusinessSelect">Business</label>                            </div>                        </div>                        <input type="hidden" value="@Model.id" name="id" />                        <div class="col-sm-12 col-md-6 col-xl-6 form-floating mb-3 text-muted">                            <input name="Businesscontact" value="@Model.businessDetail.Businesscontact" name="businesscontact" type="text" class="form-control" id="exampleFormControlInput1" placeholder="Business contact">                            <label for="exampleFormControlInput1" class="form-label ps-4">Business contact</label>                        </div>                        <div class="col-sm-12 col-md-6 col-xl-6 form-floating mb-3 text-muted">                            <input name="Email" value="@Model.businessDetail.Email" name="email" type="email" class="form-control" id="email" placeholder="Email">                            <label for="email" class="form-label ps-4">Email</label>                        </div>                        <div class="col-sm-12 col-md-6 col-xl-6 form-floating mb-3 text-muted">                            <input name="Fax" value="@Model.businessDetail.Faxnumber" name="Fax Number" type="text" class="form-control" id="exampleFormControlInput1" placeholder="Fax Number">                            <label for="exampleFormControlInput1" class="form-label ps-4">Fax Number</label>                        </div>                    </div>                    <div class="row">                        <div class="form-floating mb-3 text-muted">                            <textarea name="Detail" class="form-control" placeholder="Leave a comment here" id="floatingTextarea2" style="height: 100px"></textarea>                            <label for="floatingTextarea2" class="ps-4">Prescription or Order Detail</label>                        </div>                        <div class="col-sm-12 col-md-6 col-xl-6 form-floating mb-3 text-muted">                            <div class="form-floating mb-3">                                <select class="form-select" id="floatingSelect" aria-label="Floating label select example">                                    <option selected>Not Required</option>                                    <option value="1">Surat</option>                                    <option value="2">Two</option>                                    <option value="3">Three</option>                                </select>                                <label for="floatingSelect">Number Of Retail</label>                            </div>                        </div>                    </div>                    <div class="text-end pb-3">                        <button class="btn btn-info me-2 text-white" type="submit">Submit</button>                        <button class="btn btn-white text-info border-info" type="reset">Cancel</button>                    </div>                </div>            </form>        </div>    </div></div><script>    $('#ProfessionSelect').change(function (e) {               var profession = $('#ProfessionSelect').val();        var business = $('#BusinessSelect').val();        $.ajax({            url: "/Admin/Sendorder",            data: {'id':@Model.id, 'hproftype': profession, 'hprof': business },            type: "GET",            dataType: "html",            success: function (data) {                console.log("hello");                $("#view123").html(data);            },            error: function () {                alert("No Projects Found");                $("#tableContent").html('An error has occurred');            }        });    });        $('#BusinessSelect').change(function (e) {               var profession = $('#ProfessionSelect').val();        var business = $('#BusinessSelect').val();        $.ajax({            url: "/Admin/Sendorder",            data: { 'id': @Model.id, 'hproftype': profession, 'hprof': business },            type: "GET",            dataType: "html",            success: function (data) {                console.log("hello");                $("#view123").html(data);            },            error: function () {                alert("No Projects Found");                $("#tableContent").html('An error has occurred');            }        });    });    </script>